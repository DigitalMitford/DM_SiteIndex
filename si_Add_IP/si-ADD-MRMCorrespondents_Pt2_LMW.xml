                  <occupation type="literary" subtype="journalist editor playwright"/>
                  <occupation type="government" subtype="administrator"/>
                  <note resp="#lmw">A correspondent of <persName ref="#MRM">Mary Russell Mitford</persName><date from="1839" to="1849">between 1839 and 1849</date>.</note>
                  <note><ptr target="http://viaf.org/viaf/59892846"/></note>
               </person>
               
               <person xml:id="Kenyon_John" sex="m">
                  <persName>John Kenyon</persName>
                  <persName>
                     <surname>Kenyon</surname>
                     <forename>John</forename>
                  </persName>
